{
  "openapi": "3.0.0",
  "info": {
    "title": "keptn api",
    "version": "0.1.0"
  },
  "paths": {
    "/auth": {
      "post": {
        "tags": [
          "auth"
        ],
        "summary": "Checks the provided token",
        "operationId": "auth",
        "requestBody": {
          "required": false,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/remote"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "authenticated"
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "remote": {
        "$ref": "https://raw.githubusercontent.com/cloudevents/spec/v0.2/spec.json#/components/schemas/event"
      }
    },
    "securitySchemes": {
      "key": {
        "type": "apiKey",
        "in": "header",
        "name": "x-token"
      }
    }
  },
  "security": [
    {
      "key": []
    }
  ]
}
